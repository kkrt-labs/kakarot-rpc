{
  "cSpell.words": ["cairo", "hashrate", "KKRT", "lightclient"],
  "files.exclude": {
    "**/.git": false
  },
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.cargo.features": "all",
  "deno.enable": true,
  "deno.suggest.imports.autoDiscover": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "deno.lint": true,
  "deno.unstable": true,
  "rust-analyzer.linkedProjects": ["crates/sequencer/Cargo.toml", "./Cargo.toml"]
}
