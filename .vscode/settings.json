{
  "cSpell.words": ["cairo", "hashrate", "KKRT", "lightclient"],
  "files.exclude": {
    "**/.git": false
  },
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.cargo.features": ["testing", "hive"],
  "deno.enable": true,
  "deno.suggest.imports.autoDiscover": true,
  "[typescript]": {
    "editor.defaultFormatter": "trunk.io"
  },
  "deno.lint": true,
  "deno.unstable": true
}
